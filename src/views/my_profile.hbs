<!--My profile-->
    <!--History-->
    {{#if userLoggedIn}}
        <div class="container-fluid">
            <div class="container">
                <div class="row">
                    <div class="col-sm-3 col-xs-12 px-0">
                        <div class="nav flex-sm-column flex-row nav-pills px-0" id="profile-pills" role="tablist" aria-orientation="vertical">
                            <a class="profile-pill {{active_edit_profile}}" id="v-pills-profile-tab" data-toggle="pill" href="#v-pills-profile" role="tab" aria-controls="v-pills-profile" aria-selected="true">Thông tin cá nhân</a>
                            <a class="profile-pill {{active_status}}" id="v-pills-status-tab" data-toggle="pill" href="#v-pills-status" role="tab" aria-controls="v-pills-status" aria-selected="false">Trạng thái đơn hàng</a>
                            <a class="profile-pill {{active_history}}" id="v-pills-history-tab" data-toggle="pill" href="#v-pills-history" role="tab" aria-controls="v-pills-history" aria-selected="true">Lịch sử mua hàng</a>
                            <a class="profile-pill {{active_address}}" id="v-pills-address-tab" data-toggle="pill" href="#v-pills-address" role="tab" aria-controls="v-pills-address" aria-selected="false">Sổ địa chỉ</a>
                            <a class="profile-pill {{active_security}}" id="v-pills-security-tab" data-toggle="pill" href="#v-pills-security" role="tab" aria-controls="v-pills-security" aria-selected="false">Bảo mật</a>
                            <a class="profile-pill {{active_setting}}" id="v-pills-settings-tab" data-toggle="pill" href="#v-pills-settings" role="tab" aria-controls="v-pills-settings" aria-selected="false">Cài đặt</a>
                        </div>
                    </div>
                        <div class="col-sm-9 col-xs-12 px-0">
                            <div class="tab-content" id="profile-pills-tabContent">
                                <!--Edit profile tab content-->
                                <div class="tab-pane fade {{active_edit_profile}}" id="v-pills-profile" role="tabpanel" aria-labelledby="v-pills-profile-tab">
                                    <div class="container p-4">
                                    <form action="" method="post">
                                        <div class="container">
                                        <legend class="pb-3">Thông tin cá nhân</legend>
                                
                                        <div class="row pb-3">
                                            <div class=" col-xs-12 col-sm-4 col-md-3">
                                            <p id="login-name-label">Tên đăng nhập:</p>
                                            </div>
                                            <div class="col-xs-12 col-sm-8 col-md-9">
                                            <p id="login-name-content">E-commerce</p>
                                            </div>
                                        </div>
                                        <div class="row pb-3">
                                            <div class="col-xs-12 col-sm-4 col-md-3">
                                            <label for="fullname">Họ và tên: </label>
                                            </div>
                                            <div class="col-xs-12 col-sm-8 col-md-9">
                                            <input type="text" name="fullname" id="fullname">
                                            </div>
                                        </div>
                                        <div class="row pb-3">
                                            <div class="col-xs-12 col-sm-4 col-md-3">
                                            <label for="birthday">Ngày sinh: </label>
                                            </div>
                                            <div class="col-xs-12 col-sm-8 col-md-9">
                                            <input type="date" name="birthday" id="birthday">
                                            </div>
                                        </div>
                                        <div class="row pb-3">
                                            <div class="col-xs-12 col-sm-4 col-md-3">
                                            <label for="address">Địa chỉ: </label>
                                            </div>
                                            <div class="col-xs-12 col-sm-8 col-md-9">
                                            <textarea name="address" id="address" cols="20" rows="10"></textarea>
                                            </div>
                                        </div>
                                        <div class="row pb-3">
                                            <div class="col-xs-12 col-sm-4 col-md-3">
                                            <label for="email">Email: </label>
                                            </div>
                                            <div class="col-xs-12 col-sm-8 col-md-9">
                                            <input type="email" name="email" id="email">
                                            </div>
                                        </div>
                                        <div class="row justify-content-center">
                                            <button type="submit" class="submit-btn" id="profile-submit">Lưu</button>
                                        </div>
                                        </div>
                                    </form>
                                    </div>
                                </div>
                                <!--History tab content-->
                                <div class="tab-pane fade  {{active_history}}" id="v-pills-history" role="tabpanel" aria-labelledby="v-pills-history-tab">
                                    <div class="container" id="history_nav_bar">
                                        <div class="container-fluid" >
                                            <div class="row" >
                                                <div class="col-3 " id="date"><span>Ngày tháng năm</span></div>
                                                <div class="col-3 " id="name_product"><span>Tên hàng</span></div>
                                                <div class="col-2 " id="quantity"><span>Số lượng</span></div>
                                                <div class="col-2 " id="price"><span>Giá tiền</span></div>
                                                <div class="col-2 " id="status"><span>Trạng thái</span></div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="container" id="history">
                                        <div class="container-fluid" id="content">
                                            <div class="row">
                                                <div class="col-3" id="date"><span>13/01/2020</span></div>
                                                <div class="col-3" id="name_product"><a href="/product-detail.html">Velvet Melon High Intensity</a></div>
                                                <div class="col-2" id="quantity"><span>1</span></div>
                                                <div class="col-2" id="price"><span>$38.00</span></div>
                                                <div class="col-2" id="status"><span>Đã giao</span></div>
                                            </div>
                                            <div class="row">
                                                <div class="col-3" id="date"><span>14/01/2020</span></div>
                                                <div class="col-3" id="name_product"><a href="/product-detail.html">The expert mascaraa</a></div>
                                                <div class="col-2" id="quantity"><span>1</span></div>
                                                <div class="col-2" id="price"><span>$35.00</span></div>
                                                <div class="col-2" id="status"><span>Đang vận chuyển</span></div>
                                            </div>
                                            <div class="row">
                                                <div class="col-3" id="date"><span>16/01/2020</span></div>
                                                <div class="col-3" id="name_product"><a href="/product-detail.html">Leather shopper bag</a></div>
                                                <div class="col-2" id="quantity"><span>1</span></div>
                                                <div class="col-2" id="price"><span>$30.00</span></div>
                                                <div class="col-2" id="status"><span>Đã giao</span></div>
                                            </div>
                                            <div class="row">
                                                <div class="col-3" id="date"><span>20/01/2018</span></div>
                                                <div class="col-3" id="name_product"><a href="/product-detail.html">The expert mascaraa</a></div>
                                                <div class="col-2" id="quantity"><span>1</span></div>
                                                <div class="col-2" id="price"><span>$35.00</span></div>
                                                <div class="col-2" id="status"><span>Thanh toán</span></div>
                                            </div>
                                            <div class="row">
                                                <div class="col-3" id="date"><span>23/01/2020</span></div>
                                                <div class="col-3" id="name_product"><a href="/product-detail.html">Luxe jewel lipstick</a></div>
                                                <div class="col-2" id="quantity"><span>2</span></div>
                                                <div class="col-2" id="price"><span>$38.00</span></div>
                                                <div class="col-2" id="status"><span>Đã giao</span></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!--Status tab content-->
                                <div class="tab-pane fade {{active_status}}" id="v-pills-status" role="tabpanel" aria-labelledby="v-pills-status-tab">
                                    Nothing yet
                                </div>
                                <!--Address tab content-->
                                <div class="tab-pane fade {{active_address}}" id="v-pills-address" role="tabpanel" aria-labelledby="v-pills-address-tab">
                                    Nothing yet
                                </div>
                                <!--Security tab content-->
                                <div class="tab-pane fade {{active_security}}" id="v-pills-security" role="tabpanel" aria-labelledby="v-pills-security-tab">
                                    <div class="container p-4">                                 
                                        <form action="" method="post" id="change-pass">
                                        <div id="change-pass-fieldset">
                                            <legend class="pb-3 text-center">Thay đổi mật khẩu</legend>
                                            <div class="row pb-3 text-center">
                                            <div class="col-xs-12 col-sm-4 col-md-3">
                                                <label for="old-pass">Mật khẩu cũ: </label>
                                            </div>
                                            <div class="col-xs-12 col-sm-8 col-md-9">
                                                <input type="text" name="old-pass" id="old-pass">
                                            </div>
                                            </div>
                                            <div class="row pb-3 text-center">
                                            <div class="col-xs-12 col-sm-4 col-md-3">
                                                <label for="new-pass">Mật khẩu mới: </label>
                                            </div>
                                            <div class="col-xs-12 col-sm-8 col-md-9">
                                                <input type="text" name="new-pass" id="new-pass">
                                            </div>
                                            </div>
                                            <div class="row pb-3 justify-content-center">
                                            <button type="submit" class="submit-btn">OK</button>
                                            </div>
                                        </div>
                                        </form>
                                    </div>
                                </div>
                                <!--Setting tab content-->
                                <div class="tab-pane fade {{active_setting}}" id="v-pills-settings" role="tabpanel" aria-labelledby="v-pills-settings-tab">
                                    Nothing yet
                                </div>
                            </div>
                </div>
            </div>
        </div>
    {{else}}
        <h1 class="text-center">REQUIRE USER TO LOGIN (MOVE TO LOGIN PAGE)<br>
            <h5 class = 'text-center'><a  href="/my_profile/demo?activeTab={{page}}">click here to access my profile for demo</a></h5>
            <h3 class = 'text-center'>Đây là demo cho phân hệ user và anonymous </h3>
        </h1>
    {{/if}}
        
